{
  "name": "game-skeleton",
  "version": "1.0.0",
  "description": "MERN app in single folder",
  "main": "index.js",
  "scripts": {
    "build": "babel server --extensions .ts --out-dir build && cp -R server/public build/ && webpack --mode production",
    "start:server": "docker-compose up -d && nodemon --exec babel-node --extensions .ts server/index.ts",
    "start:client": "webpack serve --mode development",
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:prod": "NODE_ENV=production node build/index.js",
    "stop": "docker-compose down"
  },
  "keywords": [],
  "author": "Jenesh",
  "license": "UNLICENSED",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/inter": "^5.0.15",
    "@mui/joy": "^5.0.0-beta.14",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "framer-motion": "^10.16.4",
    "mongoose": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.3",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "nodemon": "^3.0.1",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
